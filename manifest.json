{
  "manifest_version": 2,

  "name": "Slack to Zendesk",
  "description": "Easily copy Slack threads Zendesk",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}
